{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "name": "Course"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true,
      "default": "Automata Language and Computation"
    },
    "Acronym": {
      "type": "string",
      "default": "ALC",
      "required": true,
      "unique": true,
      "maxLength": 10
    },
    "Description": {
      "type": "richtext"
    },
    "SubjectCode": {
      "type": "string",
      "default": "COMP5.2",
      "regex": "COMP\\d+.\\d+",
      "required": true,
      "unique": true
    },
    "lectures": {
      "collection": "lecture",
      "via": "subject"
    },
    "Instructors": {
      "via": "courses",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    }
  }
}
